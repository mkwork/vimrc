!special
*foreground:   #c5c8c6
*background:   #1d1f21
*cursorColor:  #c5c8c6

!black
*color0:       #282a2e
*color8:       #373b41

!red
*color1:       #cb555a
*color9:       #f2656b

!green
*color2:       #85cb55
*color10:      #9bed63

!yellow
*color3:       #e6cc63
*color11:      #ffe681

!blue
*color4:       #53a5ad
*color12:      #62c3cd

!magenta
*color5:       #a84466
*color13:      #d45681

!cyan
*color6:       #7bc3bd
*color14:      #94ebe4

!white
*color7:       #c5c8c6
*color15:      #c5c8c6

URxvt*font: xft:Terminus:size=12

URxvt.geometry: 150x45
URxvt.saveLines: 10000
URxvt.cutchars: "()*,<>[]{}|''"
URxvt.perl-ext-common: default,matcher,searchable-scrollback
URxvt.selection-autotransform.0: s/^([^:[:space:]]+):(\\d+):?$/+$2 \\Q$1\\E/
URxvt.skipBuiltinGlyphs: true
URxvt.url-launcher: /usr/bin/firefox
URxvt*colorUL: orange

URxvt.scrollBar: false
!URxvt.visualBell: true
! URxvt spells "eightBitInput" as "meta8" and defaults it to false
! URxvt.depth: 32
URxvt.internalBorder: 1

! Layout
rofi.width:                          50
rofi.lines:                          15
rofi.columns:                        1
rofi.color-enabled:                  true
rofi.color-normal:                   #282828,#ebdbb2,#353535,#458588,#ebdbb2
rofi.color-urgent:                   #282828,#d79921,#353535,#d79921,#282828
rofi.color-active:                   #282828,#ebdbb2,#353535,#458588,#ebdbb2
rofi.color-window:                   #282828,#458488

! Behaviour
rofi.terminal:                       urxvt
rofi.levenshtein-sort:               true
rofi.fuzzy:                          true
rofi.kb-cancel:                      Escape,Control+c

! Font settings
Xft.hintstyle:              hintfull
Xft.hinting:                1
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.lcdfilter:              lcdlight
Xft.dpi:                    96
